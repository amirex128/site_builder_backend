# Application
APP_ENV=development
APP_PORT=8080
APP_NAME=site_builder_backend
APP_URL=http://localhost:8080

# MySQL
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DATABASE=site_builder
MYSQL_USERNAME=root
MYSQL_PASSWORD=root
MYSQL_CHARSET=utf8mb4
MYSQL_COLLATION=utf8mb4_unicode_ci

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Elasticsearch
ELASTIC_HOSTS=http://localhost:9200
ELASTIC_USERNAME=
ELASTIC_PASSWORD=

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_EXCHANGE=site_builder_exchange
RABBITMQ_QUEUE=site_builder_queue

# Logging
LOG_LEVEL=debug
LOG_FILE=storage/logs/app.log

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRATION=24h

# CORS
CORS_ALLOWED_ORIGINS=*
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Content-Type,Authorization 